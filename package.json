{
  "name": "redux-firebase-user",
  "version": "0.0.1",
  "description": "User Module for a react-redux + firebase application",
  "main": "build/index.js",
  "scripts": {
    "build": "babel src -d build",
    "test": "tape -r babel-register src/**/*.spec.js src/**/**/*.spec.js | tap-spec",
    "prepublish": "npm run build",
    "release": "np"
  },
  "babel": {
    "presets": [
      "react",
      "latest",
      "stage-0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuelJung/redux-firebase-user.git"
  },
  "keywords": [
    "react",
    "redux",
    "firebase",
    "user"
  ],
  "author": "Manuel Jung",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manuelJung/redux-firebase-user/issues"
  },
  "homepage": "https://github.com/manuelJung/redux-firebase-user#readme",
  "dependencies": {
    "react": "^15.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "colortape": "^0.1.2",
    "np": "^2.13.1",
    "sinon": "^2.1.0",
    "sinon-as-promised": "^4.0.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  }
}
